{
  "name": "twitplot",
  "private": true,
  "version": "1.0.0",
  "description": "A web application that plots tweets on a map using Twitter's Search API and Google Maps Javascript API.",
  "keywords": [
    "angularjs",
    "webpack",
    "twitter",
    "googlemaps"
  ],
  "dependencies": {
    "angular": "^1.5.11",
    "angular-animate": "^1.6.5",
    "angular-mocks": "1.5.x",
    "angular-sanitize": "1.5.x",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "ng-scroll": "^1.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "js-yaml": "^3.9.1",
    "ngtemplate-loader": "^2.0.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.1",
    "webpack-dev-server": "^2.5.1"
  },
  "scripts": {
    "start": "npm run lint && webpack-dev-server --port 8000 --content-base dist/",
    "lint": "eslint app/**/*.js",
    "build": "rm -rf dist && npm run lint && webpack"
  }
}
